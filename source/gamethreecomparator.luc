module gamethreecomparator (  // Simple comparator to compare if the player's input is the same as the answer stored in gamethreeanswerrom
    input romanswer[5], 
    input playerinput[5], 
    output equal
  ) {

  always {
    equal = 0;
    if (romanswer == playerinput) {    // If equal
      equal = 1;                       // Output high signal
    }
  }
}
