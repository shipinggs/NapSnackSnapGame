module gametwosequencerom (
    input address[4],     // ROM address. It doesn't have to be 4 bits. It can be bigger to store more sequence patterns.
    output sequence[30]   // ROM output: 30-bit number that encodes the sequence of LED blinks shown on central LEDs
  ) {
 
  const SEQUENCEDATA = {30b000001010100000010000000000010,30b000000001000001010010000010100,
                        30b000001001000001001000000001001,30b000000100100001000100000010100,
                        30b000000010000001010010000010010,30b000001000100010010100000100100,
                        30b000101000000001001010000001010,30b000001001000101010010001000100,
                        30b001010010000010100010000001010,30b000010100100000101000000000010,
                        30b000100101000001000000000010101,30b000010010000000010010000010010,
                        30b000001010100001000100001001000,30b000000001000000101010000101000,
                        30b000000001000000010010000010100,30b000001000000000010100000000101};  // GBRGR. RGRGB
                           
  always {
    sequence = SEQUENCEDATA[address];
  }
}